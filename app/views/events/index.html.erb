<p style="color: green"><%= notice %></p>

<h1>Events</h1>

<div id="events">
  <% @events.each do |event| %>
    <%= render event %>
    <h2><%= event.location %></h2>
    <p>
      <%= link_to "Show this event", event, class: "btn btn-primary" %>
    </p>
  <% end %>
</div>

<%= link_to "New event", new_event_path, class: "btn btn-primary" %>
