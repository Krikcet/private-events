<h1> <%= @user.username %> </h1>
<% if @user.created_events.empty? %>
  <p> No events were created</p>
<% else %>
  <% @user.created_events.each do |event| %>
    <p><%= event.title %></p>
  <% end %>
<% end %>

<% if @user.attended_events.empty? %>
  <p>No events were attended.</p>
<% else %>
  <% @user.created_events.each do |event| %>
    <p><%= event.title %></p>
  <% end %>
<% end %>

<%= link_to 'Create event', new_event_path %>